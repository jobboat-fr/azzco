NODE_ENV=development
PORT=3000

# Comma-separated list of allowed origins for CORS.
# Example:
# FRONTEND_URL=http://localhost:5500,https://www.azzcolabs.business
FRONTEND_URL=http://localhost:5500

# AI provider key (one is enough)
GOOGLE_AI_API_KEY=
# GEMINI_API_KEY=
# AI_API_KEY=

# Optional AI settings
AI_TIMEOUT=30000
AI_CONTEXT_HISTORY_LIMIT=6

# Optional DB (recommended for production on Vercel)
# DATABASE_URL=postgresql://user:password@host:5432/dbname
# POSTGRES_URL=postgresql://user:password@host:5432/dbname

# Optional analytics security (required in production)
ANALYTICS_ADMIN_TOKEN=

# Optional geolocation
IPAPI_API_KEY=

# Optional rate limit
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100
